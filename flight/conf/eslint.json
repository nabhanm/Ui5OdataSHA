{
  "env": {
        "browser": true,
        "jquery": true,
		"qunit": true,
		"es6": true
    },
  "globals": {
		"sap": true
	},
  "rules":{  
    "no-cond-assign":"error",
    "no-console":"error",
    "no-constant-condition":"error",
    "no-control-regex":"error",
    "no-debugger":"error",
    "no-dupe-keys":"error",
    "no-empty":"error",
    "no-empty-character-class":"error",
    "no-ex-assign":"error",
    "no-extra-boolean-cast":"warn",
    "no-extra-parens":"off",
    "no-extra-semi":"error",
    "no-func-assign":"error",
    "no-inner-declarations":"error",
    "no-invalid-regexp":"error",
    "no-unsafe-negation":"error",
    "no-obj-calls":"error",
    "no-regex-spaces":"error",
    "no-sparse-arrays":"error",
    "no-unreachable":"error",
    "use-isnan":"error",
    "valid-jsdoc": ["warn",{"requireReturn": false}],
    "valid-typeof":"error",
    "block-scoped-var":"error",
    "complexity":"off",
    "consistent-return":"warn",
    "curly":"error",
    "default-case":"warn",
    "dot-notation":"off",
    "eqeqeq":"warn",
    "guard-for-in":"error",
    "no-alert":"error",
    "no-caller":"error",
    "no-div-regex":"error",
    "no-else-return":"off",
	"no-eq-null":"off",
    "no-eval":"error",
    "no-extend-native":"error",
    "no-fallthrough":"error",
    "no-floating-decimal":"error",
    "no-implied-eval":"error",
	"no-labels":"error",
    "no-iterator":"error",
    "no-lone-blocks":"error",
    "no-loop-func":"error",
    "no-multi-str":"error",
    "no-global-assign":"error",
    "no-new":"error",
    "no-new-func":"error",
    "no-new-wrappers":"warn",
    "no-octal":"error",
    "no-octal-escape":"error",
    "no-proto":"error",
    "no-redeclare":"warn",
    "no-return-assign":"error",
    "no-script-url":"error",
    "no-self-compare":"error",
    "no-sequences":"error",
    "no-unused-expressions":"warn",
    "no-warning-comments":"warn",
    "no-with":"error",
    "radix":"error",
    "wrap-iife":"error",
    "yoda":"error",
    "strict":["error", "function"],
    "no-catch-shadow":"error",
    "no-delete-var":"error",
    "no-label-var":"error",
    "no-shadow":"error",
    "no-shadow-restricted-names":"error",
    "no-undef":"off",
    "no-undefined":"off",
    "no-undef-init":"error",
    "no-unused-vars": ["error", {"vars":"all", "args":"none"}],
    "no-use-before-define":"warn",
    "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
    "camelcase":"warn",
	"comma-dangle":"warn",
    "consistent-this":"error",
    "eol-last":"off",
    "func-names":"off",
    "func-style":"off",
    "new-cap":"off",
    "new-parens":"error",
    "no-nested-ternary":"error",
    "no-array-constructor":"error",
    "no-lonely-if":"error",
    "no-new-object":"error",
	"func-call-spacing":"error",
	"no-plusplus":"off",
    "semi-spacing":"error",
    "no-ternary":"off",
    "no-trailing-spaces":"off",
    "no-underscore-dangle":"off",
    "no-mixed-spaces-and-tabs":["error", "smart-tabs"],
    "quotes":"off",
    "quote-props":"off",
    "semi":"error",
    "sort-vars":"off",
    "keyword-spacing":"error",
    "space-infix-ops":"error",
    "object-curly-spacing":"off",
	"array-bracket-spacing":"off",
    "space-unary-ops":"error",
    "max-nested-callbacks":"warn",
    "one-var":"off",
    "wrap-regex":"off",
    "max-depth":"off",
    "max-len":"off",
    "max-params":"off",
    "max-statements":"off",
    "no-bitwise":"off",
    "handle-callback-err":"off",
    "no-mixed-requires":"off",
    "no-new-require":"off",
    "no-path-concat":"off",
    "no-process-exit":"off",
    "no-restricted-modules":"off",
    "no-sync":"off",
	/*SAP recommended Fiori rules starts from here*/				
					"sap-cross-application-navigation":"warn",
					"sap-forbidden-window-property":"warn",
					"sap-message-toast":"warn",
					"sap-no-absolute-component-path":"error",
					"sap-no-br-on-return":"error",
					"sap-no-dom-access":"warn",
					"sap-no-dom-insertion":"warn",
					"sap-no-dynamic-style-insertion":"error",
					"sap-no-element-creation":"error",
					"sap-no-encode-file-service":"warn",
					"sap-no-exec-command":"error",
					"sap-no-global-define":"error",
					"sap-no-global-event":"error",
					"sap-no-global-selection":"warn",
					"sap-no-global-variable":"error",
					"sap-no-hardcoded-color":"error",
					"sap-no-hardcoded-url":"error",
					"sap-no-history-manipulation":"warn",
					"sap-no-jquery-device-api":"error",
					"sap-no-localhost":"warn",
					"sap-no-localstorage":"error",
					"sap-no-location-reload":"error",
					"sap-no-location-usage":"warn",
					"sap-no-navigator":"off",
					"sap-no-override-rendering":"error",
					"sap-no-override-storage-prototype":"error",
					"sap-no-proprietary-browser-api":"warn",
					"sap-no-sessionstorage":"error",
					"sap-no-ui5-prop-warning":"off",
					"sap-no-ui5base-prop":"off",
					"sap-timeout-usage":"warn",
					"sap-ui5-no-private-prop":"warn",
					"sap-usage-basemastercontroller":"warn",
	/*Custom rules starts from here*/
			"no-multi-object-properties-one-line": "warn",
			"no-multi-object-properties-first-line": "warn",
			"no-multi-object-properties-last-line": "warn",
			"no-object-prop-key-value-split": "warn",
			"prefer-filter": "off",
			"no-instanceof-array": "warn",
			"no-not-not": "warn",
			"no-unneeded-match": "warn",
			"augmented-assignment": "off",
			"simplify-boolean-expression": "warn",
			"valid-define": "warn",
			"template-string": "off",
			"no-func-space": "off",
			"no-EventBus": "warn",
			"no-ActiveXObject": "warn",
			"no-oEvent-preventDefault": "warn",
			"no-oEvent-stopPropagation": "warn",
			"no-hardcodedId": "off"
	}
}